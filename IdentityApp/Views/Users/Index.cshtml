@model IEnumerable<IdentityUser>

<div class="row">
    <div class="col-10">
        <h2>Users</h2>
    </div>
    <div class="col-2 text-end">
        <a class="btn btn-sm btn-success" asp-action="Create">Create User</a>
    </div>
</div>
<hr>
<table class="table table-sm table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Option</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>
                    <a href="" class="btn btn-primary btn-sm">Edit</a>
                    <a href="" class="btn btn-danger btn-sm">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>